{
  "name": "n8n-nodes-typesense",
  "version": "0.1.0",
  "description": "n8n community node for Typesense",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "typesense"
  ],
  "license": "MIT",
  "author": "Max Soukhomlinov",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msoukhomlinov/n8n-nodes-typesense.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts",
    "format": "prettier --write .",
    "copy:assets": "copyfiles \"nodes/**/*.svg\" dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "tslib": "^2.7.0",
    "typesense": "^2.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "cross-env": "^7.0.3",
    "copyfiles": "^2.4.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
