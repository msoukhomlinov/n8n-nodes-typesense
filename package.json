{
  "name": "n8n-nodes-typesense",
  "version": "1.0.0",
  "description": "n8n community node for Typesense",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "typesense"
  ],
  "license": "MIT",
  "author": {
    "name": "Max Soukhomlinov",
    "url": "https://github.com/msoukhomlinov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msoukhomlinov/n8n-nodes-typesense.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TypesenseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Typesense/Typesense.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "lint": "eslint .",
    "format": "prettier --write .",
    "copy:assets": "copyfiles \"nodes/**/*.svg\" dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "n8n-core": "^1.107.4",
    "n8n-workflow": "^1.107.4",
    "tslib": "^2.8.1",
    "typesense": "^2.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n8n-nodes-base": "^1.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "cross-env": "^10.0.0",
    "copyfiles": "^2.4.1",
    "prettier": "^3.6.2",
    "typescript": "^5.5.4"
  }
}
